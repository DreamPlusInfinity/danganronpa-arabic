msgid "<CLT 26>We need some way besides the face...<CLT>"
msgstr ""

msgctxt "0009"
msgid "<CLT 26>We have to find out who it was!<CLT>"
msgstr ""

msgctxt "0010"
msgid "<CLT 26>The victim...is a ghost!<CLT>"
msgstr ""

msgctxt "0011"
msgid "<CLT 26>Do you still not understand?<CLT>"
msgstr ""

